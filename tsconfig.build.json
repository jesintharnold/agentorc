{
  "compilerOptions": {
  "incremental": true,
  "target": "es2020",
  "module": "commonjs",
  "declaration": true,
  "sourceMap": true,
  "strict": true,
  "moduleResolution": "node",
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "composite": true,
  "rootDir": ".",
  "outDir": "./dist/",
  "resolveJsonModule": true
  },
  "include": ["./**/*","./**/*.yaml","./**/*.json"],
  "exclude": ["dist"]
}